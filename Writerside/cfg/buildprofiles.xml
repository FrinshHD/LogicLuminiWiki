<buildprofiles xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="https://resources.jetbrains.com/writerside/1.0/build-profiles.xsd">
    <variables>
        <primary-color>#54176b</primary-color>
        <product-web-url>%mainSiteUrl%</product-web-url>
        <color-preset>soft</color-preset>
        <link-color>#712a8c</link-color>

        <custom-favicons>logo.png</custom-favicons>
        <header-logo>logo.png</header-logo>

        <download-title>Join our Discord</download-title>
        <download-page>%mainSiteUrl%discord</download-page>
        <showDownloadButton>true</showDownloadButton>
        <noindex-content>false</noindex-content>
    </variables>
    <footer>
        <copyright>2024 CodeArray Development</copyright>
        <social type="youtube" href="https://www.youtube.com/@frinshhd"/>
        <link href="%mainSiteUrl%legalnotice.html">Legal Notice</link>
        <link href="%mainSiteUrl%privacypolicy.html">Privacy Policy</link>
    </footer>
</buildprofiles>